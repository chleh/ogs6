<?xml version="1.0" encoding="ISO-8859-1"?>
<OpenGeoSysProject>
    <mesh axially_symmetric="true">pipe.vtu</mesh>
    <geometry>pipe.gml</geometry>
    <processes>
        <process>
            <name>stokes_brinkman</name>
            <type>INCOMPRESSIBLE_STOKES_BRINKMAN</type>
            <integration_order>3</integration_order>
            <dimension>2</dimension>

            <bed_radius>            1 </bed_radius>

            <!-- they don't matter here -->
            <pellet_diameter>       1 </pellet_diameter>
            <average_darcy_velocity>0.5 </average_darcy_velocity>
            <fluid_density>         1  </fluid_density>
            <fluid_viscosity>       1  </fluid_viscosity>

            <process_variables>
                <velocity>darcy_velocity</velocity>
                <pressure>pressure</pressure>
            </process_variables>

            <secondary_variables>
            </secondary_variables>
        </process>
    </processes>
    <time_loop>
        <processes>
            <process ref="stokes_brinkman">
                <nonlinear_solver>basic_picard</nonlinear_solver>
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <abstol>1e-10</abstol>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <output>
                    <variables>
                        <variable>darcy_velocity</variable>
                        <variable>pressure</variable>
                    </variables>
                </output>
                <time_stepping>
                    <type>SingleStep</type>
                </time_stepping>
            </process>
        </processes>
        <output>
            <type>VTK</type>
            <prefix>pipe</prefix>
            <timesteps>
                <pair>
                    <repeat>1</repeat>
                    <each_steps>1</each_steps>
                </pair>
            </timesteps>
        </output>
    </time_loop>
    <parameters>
        <parameter>
            <name>porosity</name>
            <type>Constant</type>
            <value>1</value>
        </parameter>
        <parameter>
            <name>mu_eff</name>
            <type>Constant</type>
            <value>1</value>
        </parameter>
        <parameter>
            <name>lambda_eff</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>f_1</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>f_2</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>

        <parameter>
            <name>velocity_0</name>
            <type>Constant</type>
            <values>0 0</values>
        </parameter>

        <parameter>
            <name>pressure_0</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>

        <parameter>
            <name>zero</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>

        <parameter>
            <name>one</name>
            <type>Constant</type>
            <value>1</value>
        </parameter>
    </parameters>
    <process_variables>
        <process_variable>
            <name>darcy_velocity</name>
            <components>2</components>
            <order>2</order>
            <initial_condition>velocity_0</initial_condition>

            <boundary_conditions>
                <boundary_condition>
                    <geometrical_set>pipe</geometrical_set>
                    <geometry>inner</geometry>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>zero</parameter>
                </boundary_condition>

                <boundary_condition>
                    <geometrical_set>pipe</geometrical_set>
                    <geometry>bottom</geometry>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>zero</parameter>
                </boundary_condition>

                <boundary_condition>
                    <geometrical_set>pipe</geometrical_set>
                    <geometry>outer</geometry>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>zero</parameter>
                </boundary_condition>
                <boundary_condition>
                    <geometrical_set>pipe</geometrical_set>
                    <geometry>outer</geometry>
                    <type>Dirichlet</type>
                    <component>1</component>
                    <parameter>zero</parameter>
                </boundary_condition>

                <boundary_condition>
                    <geometrical_set>pipe</geometrical_set>
                    <geometry>top</geometry>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>zero</parameter>
                </boundary_condition>
                <boundary_condition>
                    <geometrical_set>pipe</geometrical_set>
                    <geometry>top</geometry>

                    <type>NonuniformDirichlet</type>
                    <mesh>pipe_bc_inlet.vtu</mesh>
                    <field_name>velocity_y_inlet</field_name>

                    <component>1</component>
                    <!-- <parameter>one</parameter> -->
                </boundary_condition>
            </boundary_conditions>
        </process_variable>

        <process_variable>
            <name>pressure</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>pressure_0</initial_condition>

            <boundary_conditions>
                <boundary_condition>
                    <geometrical_set>pipe</geometrical_set>
                    <geometry>bottom</geometry>
                    <type>Dirichlet</type>
                    <component>0</component>
                    <parameter>zero</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>

    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>basic_picard</name>
            <type>Picard</type>
            <max_iter>50</max_iter>
            <linear_solver>general_linear_solver</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>general_linear_solver</name>
            <!-- <lis>-i bicgstab -p ilu -tol 1e-10 -maxiter 10000</lis> -->
            <lis>-i cg -p jacobi -tol 1e-10 -maxiter 10000</lis>
            <eigen>
                <solver_type>SparseLU</solver_type>
                <precon_type>DIAGONAL</precon_type>
                <max_iteration_step>10000</max_iteration_step>
                <error_tolerance>1e-10</error_tolerance>
            </eigen>
        </linear_solver>
    </linear_solvers>
</OpenGeoSysProject>
